{
  "name": "room-assistant",
  "version": "2.9.1",
  "description": "Presence tracking and more for automation on the room-level",
  "author": "Heiko Rothe <me@heikorothe.com> (https://heikorothe.com)",
  "license": "MIT",
  "repository": "github:mKeRix/room-assistant",
  "keywords": [
    "smart home",
    "home automation",
    "home assistant",
    "presence",
    "bluetooth",
    "thermopile"
  ],
  "homepage": "https://www.room-assistant.io",
  "bugs": "https://github.com/mKeRix/room-assistant/issues",
  "funding": "https://github.com/mKeRix/room-assistant?sponsor=1",
  "bin": "./bin/room-assistant.js",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "files": [
    "dist/**/*.{js,ts}",
    "bin/*.js",
    ".github/FUNDING.yml"
  ],
  "scripts": {
    "prepack": "npm run build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postinstall": "github-sponsors",
    "format": "prettier --write \"src/**/*.ts\" \"bin/*.js\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test,bin}/**/*.{ts,js}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/terminus": "^7.0.1",
    "async-mqtt": "^2.6.1",
    "chalk": "^4.1.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "config": "^3.3.1",
    "democracy": "^3.1.3",
    "github-sponsors": "^1.0.1",
    "js-yaml": "^3.14.0",
    "kalmanjs": "^1.1.0",
    "lodash": "^4.17.20",
    "mathjs": "^7.2.0",
    "nest-emitter": "^1.1.1",
    "nest-winston": "^1.3.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "slugify": "^1.4.5",
    "swagger-ui-express": "^4.1.4",
    "systeminformation": "^4.27.3",
    "update-notifier": "^4.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.1",
    "@nestjs/testing": "^7.4.4",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.8",
    "@types/i2c-bus": "^5.1.0",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/mathjs": "^6.0.5",
    "@types/mdns": "0.0.33",
    "@types/noble": "0.0.40",
    "@types/node": "^14.6.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vuepress/plugin-pwa": "^1.5.4",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2",
    "vuepress": "^1.5.4",
    "vuepress-plugin-seo": "^0.1.4",
    "vuepress-plugin-sitemap": "^2.3.1"
  },
  "optionalDependencies": {
    "@abandonware/noble": "^1.9.2-5",
    "canvas": "^2.6.1",
    "i2c-bus": "^5.1.0",
    "mdns": "^2.5.1",
    "onoff": "^6.0.0"
  },
  "engines": {
    "node": ">= 10 < 13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"src/**/*.ts\" --pattern \"bin/*.js\"",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./env.ts"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
